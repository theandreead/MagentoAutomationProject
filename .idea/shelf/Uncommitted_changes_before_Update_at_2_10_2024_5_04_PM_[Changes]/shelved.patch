+    private final Waiters waiters = new Waiters(driver);
+
+    By phoneNumber = By.id("telephone");
+    By streetAddress = By.id("street_1");
+    By city = By.id("city");
+    By stateList = By.id("region_id");
+    By zipCode = By.id("zip");
+    By countryList = By.id("country");
